                                    </DockPanel>
                                    <DockPanel Style="{StaticResource MenuDockPanel}">
                                        <fa:ImageAwesome
                                            Width="13"
                                            Margin="10"
                                            Foreground="#8E8F96"
                                            Icon="Folder" />
                                        <TextBlock Margin="10" Text="Kanallar ve Roller" />
                                    </DockPanel>
                                </StackPanel>
                            </Border>
                            <TreeView
                                Margin="0,10"
                                Background="Transparent"
                                BorderThickness="0">
                                <TreeViewItem Header="README.TXT" IsExpanded="True">
                                    <DockPanel Margin="-20,5">
                                        <fa:ImageAwesome
                                            Width="15"
                                            Foreground="#80848E"
                                            Icon="Book" />
                                        <TreeViewItem Padding="0" Header="information" />
                                    </DockPanel>
                                    <DockPanel Margin="-20,5">
                                        <fa:ImageAwesome
                                            Width="15"
                                            Foreground="#80848E"
                                            Icon="Bullhorn" />
                                        <TreeViewItem Padding="0" Header="announcements" />
                                    </DockPanel>

                                </TreeViewItem>
                            </TreeView>

                            <TreeView
                                Margin="0,10"
                                Background="Transparent"
                                BorderThickness="0">
                                <TreeViewItem Header="RYTHM HALL" IsExpanded="True">
                                    <DockPanel Margin="-20,5">
                                        <fa:ImageAwesome
                                            Width="15"
                                            Foreground="#80848E"
                                            Icon="Bullhorn" />
                                        <TreeViewItem Padding="0" Header="event-announcements" />
                                    </DockPanel>
                                    <DockPanel Margin="-20,5">
                                        <fa:ImageAwesome
                                            Width="15"
                                            Foreground="#80848E"
                                            Icon="Hashtag" />
                                        <TreeViewItem Padding="0" Header="rythm-lounge" />
                                    </DockPanel>
                                    <DockPanel Margin="-20,5">
                                        <fa:ImageAwesome
                                            Width="15"
                                            Foreground="#80848E"
                                            Icon="Hashtag" />
