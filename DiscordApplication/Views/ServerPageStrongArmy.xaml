                                            Width="15"
                                            Foreground="#80848E"
                                            Icon="Hashtag" />
                                        <TreeViewItem Padding="0" Header="off-topic" />
                                    </DockPanel>
                                    <DockPanel Margin="-20,5">
                                        <fa:ImageAwesome
                                            Width="15"
                                            Foreground="#80848E"
                                            Icon="Hashtag" />
                                        <TreeViewItem Padding="0" Header="music-sharing" />
                                    </DockPanel>

                                    <DockPanel Margin="-20,5">
                                        <fa:ImageAwesome
                                            Width="15"
                                            Foreground="#80848E"
                                            Icon="Hashtag" />
                                        <RadioButton
                                            x:Name="ServerChat"
                                            Margin="8,0"
                                            Click="PageNavigation_Click"
                                            Content="bot-commands">
                                            <RadioButton.Template>
                                                <ControlTemplate TargetType="{x:Type RadioButton}">
                                                    <Grid Background="{TemplateBinding Background}">
                                                        <ContentPresenter
                                                            Margin="10,5"
                                                            HorizontalAlignment="Center"
                                                            VerticalAlignment="Center" />
                                                    </Grid>
                                                </ControlTemplate>
                                            </RadioButton.Template>
                                        </RadioButton>
                                    </DockPanel>
                                    <DockPanel Margin="-20,5">
                                        <fa:ImageAwesome
                                            Width="15"
                                            Margin="0,0,5,0"
                                            Foreground="#80848E"
                                            Icon="Lock" />
                                        <TreeViewItem
                                            Margin="-5,0"
                                            Padding="0"
                                            Header="Vibes" />
                                    </DockPanel>
                                </TreeViewItem>
                            </TreeView>

                            <Border
                                Margin="0,10"
                                Padding="5"
                                BorderBrush="#3B3D44"
                                BorderThickness="0,1,0,0">
