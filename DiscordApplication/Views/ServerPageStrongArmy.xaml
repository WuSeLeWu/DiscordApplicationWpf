                                            Padding="0"
                                            Header="Rythm Radio"
                                            IsExpanded="true">
                                            <DockPanel Margin="-20,15,0,5">
                                                <fa:ImageAwesome
                                                    Width="15"
                                                    Foreground="White"
                                                    Icon="QuestionCircle" />
