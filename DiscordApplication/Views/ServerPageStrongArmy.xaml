                                </Border.Background>
                                <dxa:AccordionControl Background="Transparent">
                                    <DockPanel>
                                        <Border
                                            Width="20"
                                            Height="20"
                                            Background="white"
                                            CornerRadius="60">
                                            <fa:ImageAwesome
                                                Width="20"
                                                Foreground="Green"
                                                Icon="CheckCircle" />
                                        </Border>
                                        <dxa:AccordionItem
                                            Foreground="white"
                                            Header="Strong Army"
                                            IsExpanded="True" />
                                    </DockPanel>
                                </dxa:AccordionControl>
                            </Border>


                        </StackPanel>
                        <StackPanel>
                            <Border
                                Margin="8,10"
                                Padding="5"
                                BorderBrush="#3B3D44"
                                BorderThickness="0,0,0,1">
                                <StackPanel>
                                    <DockPanel Style="{StaticResource MenuDockPanel}">
                                        <fa:ImageAwesome
                                            Width="13"
                                            Margin="10"
                                            Foreground="#8E8F96"
                                            Icon="MapSigns" />
                                        <TextBlock Margin="10" Text="Sunucu Rehberi" />
                                    </DockPanel>
                                    <DockPanel Style="{StaticResource MenuDockPanel}">
                                        <fa:ImageAwesome
                                            Width="13"
                                            Margin="10"
                                            Foreground="#8E8F96"
                                            Icon="StickyNote" />
